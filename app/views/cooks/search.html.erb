<div class="container-area">
    <div class="container-box">
        
        <h1>検索結果</h1>
        <h6>メニュー数 : <%= @results.count%></h6>
        <% case @path%>
            <% when '/cooks/search?q%5Bstandby_cont%5D=%E6%BA%96%E5%82%99OK&commit=%E6%A4%9C%E7%B4%A2' %>
                <h3>準備OK</h3>
            <% when '/cooks/search?q%5Bcookname_or_recom_cont%5D=%E3%81%BF%E3%81%A3%E3%81%9F%E3%82%93%E3%81%8A%E6%B0%97%E3%81%AB%E5%85%A5%E3%82%8A&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>みったん推し</h3>
            <% when '/cooks/search?q%5Bcookname_or_recom_cont%5D=%E3%81%BE%E3%81%A1%E3%81%8A%E6%B0%97%E3%81%AB%E5%85%A5%E3%82%8A&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>まち推し</h3>
            <% when '/cooks/search?q%5Bstandby_cont%5D=%E6%9C%AA%E5%AE%9A&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>未定</h3>
            <% when '/cooks/search?q%5Bstandby_cont%5D=%E6%B8%88%E3%81%BF&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>済み</h3>
            <% when '/cooks/search?q%5Bcookname_or_cost_cont%5D=%E5%AE%89%E4%B8%8A%E3%81%8C%E3%82%8A&commit=%E5%AE%89%E4%B8%8A%E3%81%8C%E3%82%8A'%>
                <h3>コスパ</h3>
            <% when '/cooks/search?q%5Bcookname_or_cost_cont%5D=%E7%B0%A1%E5%8D%98&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>簡単</h3>
             <% when '/cooks/search?q%5Bcookname_or_cost_cont%5D=%E8%B1%AA%E8%8F%AF&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>豪華</h3>
            <% when '/cooks/search?q%5Bcookname_or_cost_cont%5D=%E6%89%8B%E3%81%AE%E8%BE%BC%E3%82%93%E3%81%A0&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>手込み</h3>
            <% when '/cooks/search?q%5Bcookname_or_cost_cont%5D=%E4%BD%9C%E3%82%8A%E7%BD%AE%E3%81%8D%E5%8F%AF&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>作置き</h3>
            <% when '/cooks/search?q%5Brecom_cont%5D=2%E4%BA%BA%E5%85%B1%E3%81%8A%E6%B0%97%E3%81%AB%E5%85%A5%E3%82%8A&commit=%E6%A4%9C%E7%B4%A2'%>
                <h3>2人推し</h3>            
        <% else %>
            <h3></h3>
        <% end %>
        <br>
        <h6>他のおすすめを見る</h6>

        <div class = "recom_box">
            <div class = "area-box">
                <%= link_to cooks_path do %>
                    <%= image_tag 'data:image/jpeg;base64,' ,class: 'button-image'%>
                    <p>一覧</p>
                <% end %>
            </div>
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Bstandby_cont%5D=未定&commit=検索" do %>
                <%= image_tag 'data:image/jpeg;base64,' ,class: 'button-image'%>
                    <p>未定</p>
                <% end %>
            </div>
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Bstandby_cont%5D=済み&commit=検索" do %>
                    <%= image_tag 'data:image/jpeg;base64,' ,class: 'button-image'%>
                    <p>済み</p>
                <% end %>
            </div>
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Bcookname_or_cost_cont%5D=安上がり&commit=安上がり" do %>
                    <%= image_tag 'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTCqF8K5ikFmy8_gpp4rJnqShb3YQbgT8-kIQ&usqp=CAU' ,class: 'button-image'%>
                    <p>コスパ</p>
                <% end %>
            </div>
        </div>
        <div class = "recom_box">
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Bcookname_or_cost_cont%5D=簡単&commit=検索" do %>
                    <%= image_tag 'data:image/jpeg;base64,' ,class: 'button-image'%>
                    <p>簡単</p>
                <% end %>
            </div>
            <br>
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Bcookname_or_cost_cont%5D=豪華&commit=検索" do %>
                    <%= image_tag 'https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQLh3sIa7GvgNdGfWZweWRo8TUHNqTX5oHYYw&usqp=CAU' ,class: 'button-image'%>
                    <p>豪華</p>
                <% end %>
            </div>
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Bcookname_or_cost_cont%5D=手の込んだ&commit=検索" do %>
                    <%= image_tag 'data:image/jpeg;base64,' ,class: 'button-image'%>
                    <p>手込み</p>
                <% end %>
            </div>
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Bcookname_or_cost_cont%5D=作り置き可&commit=検索" do %>
                    <%= image_tag 'data:image/jpeg;base64,' ,class: 'button-image'%>
                    <p>作置き</p>
                <% end %>
            </div>
            <div class = "area-box">
                <%= link_to "/cooks/search?q%5Brecom_cont%5D=2人共お気に入り&commit=検索" do %>
                    <%= image_tag 'data:image/jpeg;base64,' ,class: 'button-image'%>
                    <p>2人推し</p>
                <% end %>
            </div>
            <div class = "area-box">
                <%= link_to families_path do %>
                    <%= image_tag  'data:image/jpeg;base64,',class: 'button-image'%>
                    <p>良い子</p>
                <% end %>
            </div>
        </div>
        <!--ここまでOK-->
        <br>
         <div class="index-comment-area">
            <% @results.each do |cook| %>
            <div class="index-comment-box">
            <div class = "boxbr">
                </div>
            <div class = "boxleft">
                <% if cook.image.attached? %>
                    <%= image_tag cook.image ,class: 'cook-image' %>
                <% else %>
                    <%= image_tag 'no-image.user.png' ,class: 'cook-image'%>
                <% end %>
                </div>
                <div class = "boxright">
                    <h3><%= cook.cookname %></h3>
                    <p class = "url"><%= Rinku.auto_link(cook.cookurl, :all, 'target="_blank"').html_safe %></p>
                    <p><%= cook.standby %></p>
                    <p><%= cook.id %>
                    <%= link_to '詳細', cook %>
                    <%= link_to '編集', edit_cook_path(cook) %>
                    <%= link_to '削除', cook, method: :delete, data: { confirm: 'Are you sure?' } %></p>
                    <br>
                </div>
                </div>
                <% end %>
                <div class = "boxbr">
                </div>

            </div>
        </div>
        <br>
        <%= link_to 'メニューを追加する', new_cook_path %>

    </div>
</div>
