    <%= link_to  walk_dogs_path do%>
      <%= image_tag "data:image/png;base64,",class: 'yayo-image'%>
      <% end %>
  <% elsif (@min == "15") || (@min == "45")  %>
    <%= link_to  snack_dogs_path  do%>
